{
  "expo": {
    "name": "mina_app",
    "slug": "mina_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mina-app-icon.png",
    "scheme": "mina-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mina-ios",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.minaios"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": "1.0.0",
    "privacy": "public",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a6ccc5ec-c71b-43b1-86d4-d617391fd941"
      }
    },
    "owner": "mina-app",
    "updates": {
      "url": "https://u.expo.dev/a6ccc5ec-c71b-43b1-86d4-d617391fd941"
    }
  }
}
